      <procedures_CoreProceduresItem id="1325" key="9f4b600d-a202-4f26-bec3-866e8f6900bf" parentID="1324" level="3" creatorID="0" sortOrder="12" createDate="2017-07-17T11:15:07" updateDate="2017-07-18T14:36:50" nodeName="Responding to Abuse and Neglect" urlName="responding-to-abuse-and-neglect" path="-1,1154,1324,1325" isDoc="" nodeType="1321" creatorName="Webteam" writerName="Webteam" writerID="0" template="1320" nodeTypeAlias="procedures_CoreProceduresItem">
